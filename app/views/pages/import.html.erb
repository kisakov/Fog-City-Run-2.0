<!DOCTYPE html>
<html>
<head>
  <title>Racers</title>
</head>
<nav class="navbar navbar-default navbar-fixed-top">
  <a class="navbar-brand" href="#"><b>Fog City Run</b></a>
  <ul class="nav navbar-nav">
    <li class="nav-item">
      <a class="nav-link" <%= link_to "Home", root_path %></a>
    </li>
    <li class="nav-item">
      <a class="nav-link" <%= link_to 'Racers', racers_path %></a>
    </li>
    <li class="nav-item">
      <a class="nav-link" <%= link_to 'Races', races_path %></a>
    </li>
    <li class="nav-item">
      <a class="nav-link" <%= link_to 'Racer Count', count_path %></a>
    </li>
    <li class="nav-item active">
      <a class="nav-link" <%= link_to 'Import', import_path %></a>
    </li>
  </ul>
</nav>
<body style="padding-top: 60px">
<div class="container-fluid">
  <div class="row">
      <h4> Import Racers</h4>
      <%= form_tag import_racers_path, multipart: true do %>
        <%=file_field_tag :file%>
        <%=submit_tag "Import CSV"%>
      <% end %>
  </div>
  <div class="row">
      <h4> Import Races</h4>
      <%= form_tag import_races_path, multipart: true do %>
        <%=file_field_tag :file%>
        <%=submit_tag "Import CSV"%>
      <% end %>
  </div>
  <div class="row">
      <h4> Import Results</h4>
      <%= form_tag import_results_path, multipart: true do %>
        <%=file_field_tag :file%>
        <%=submit_tag "Import CSV"%>
      <% end %>
  </div>
</div>
</body>
</html>